/* Toard.css - A simple, yet easy to use css. */
/* https://github.com/Yonle/Toard.css */

h1, h2, h3, h4 {
  margin: 0;
}

pre {
  margin-top: 0;
  white-space: -moz-pre-wrap; /* Mozilla, supported since 1999 */
  white-space: -pre-wrap; /* Opera */
  white-space: -o-pre-wrap; /* Opera */
  white-space: pre-wrap; /* CSS3 - Text module (Candidate Recommendation) http://www.w3.org/TR/css3-text/#white-space */
  word-wrap: break-word; /* IE 5.5+ */
}

.border {
  border: solid 1px;
  border-radius: 2px;
  background: white;
}

.fixed {
  position: fixed;
}

.absolute {
  position: absolute;
}

.relative {
  position: relative;
}

.grid {
  display: grid;
  width: auto;
}

.flex {
  display: flex;
}

.bottom {
  bottom: 5px;
}

.top {
  top: 5px;
}

.left {
  float: left;
  left: 5px;
}

.right {
  float: right;
  right: 5px;
}

.clear_left {
  clear: left;
}

.clear_right {
  clear: right;
}

.popup {
  visibility: hidden;
  z-index: 4;
}

.popup:target {
  visibility: visible;
}
</style>
<style>
}
@font-face {
  font-family: ubuntu-regular;
  src: url(/Ubuntu-R.ttf);
}

body {
  font-family: helvetica;
}

video {
  position: sticky;
  top: 0;
  z-index: 3;
  width: 100%;
  max-height: 60vh;
}

/* img only shows up for thumbnail, and theyâ€™re completely small. */
img {
  max-width: 150px;
}

a {
  color: black;
}

#sbox:hover, #sbtn:hover + #sbox {
  visibility: visible;
}

#player {
  width: 60%;
  z-index: -1;
}

#relatedVideos {
  z-index: -1;
  position: static;
}

.rVideo {
  display: flex;
  padding: 3px;
}

.rVideoInfo {
  display: block;
  padding: 10px;
}

.rVideoInfo img {
  float: left;
}

/* Channel Info */
.chInf {
  border: solid 1px;
  border-radius: 2px;
  max-width: 72%;
  display: flex;
}

/* Mobile View */
@media only screen and (max-width: 960px) {
  body {
    display: block;
    margin: 0;
  }

  #player, #vidInfo {
    position: static;
    background: white;
    margin: 0;
    top: 0;
    left: 0;
    width: 100%;
    max-width: 100%;
  }

  #relatedVideos {
    float: left;
    position: relative;
    padding: 2px;
  }
  
  .rVideo, #chnInf, #vidInfo {
    position: relative;
    padding: 2.5px;
  }
  
  .rVideoInfo {
    width: 170px;
  }
  
  .img {
    max-width: 200px;
    height: auto;
  }
  
  video {
    width: 100%;
    max-width: 100%;
  }
}
</style>
